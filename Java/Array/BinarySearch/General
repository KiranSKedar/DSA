TC - o(logn)

package in.sp.binarySearch;

public class BinarySearch {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int[] arr= {1,2,3,4,5,6};
		int n=arr.length;
		int low=0,high=n-1;
		int target=4;
		
		while(low<=high) {
			int mid=(low+high)/2;
			if(arr[mid]==target) {
				
				System.out.println("Element found at index:-"+mid);
				break;
			}
			else if(arr[mid]>target)
				high=mid-1;
			else
				low=mid+1;
		}
		
	}

}
